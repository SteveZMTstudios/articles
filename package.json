{
  "name": "hexo-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "deploy:firebase": "npm run build && npx firebase deploy --only hosting --project your-project-id",
    "server": "hexo server",
    "test": "hexo clean && hexo generate && hexo server --drafts --watch"
  },
  "hexo": {
    "version": "8.1.1"
  },
  "overrides": {
    "hexo-offline": {
      "hexo": "$hexo"
    },
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
    "glob": "^11.0.0",
    "source-map": "^0.7.6"
  },
  "resolutions": {
    "sourcemap-codec": "^1.5.0"
  },
  "dependencies": {
    "hexo": "^8.1.1",
    "hexo-algolia": "^1.3.2",
    "hexo-cli": "^4.3.2",
    "hexo-deployer-git": "^4.0.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-search": "^2.4.3",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-helper-qrcode": "^1.0.2",
    "hexo-lazyload-image": "^1.1.3",
    "hexo-offline": "^4.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^7.0.1",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "mdui": "^2.1.4",
    "qiniu": "^7.14.0",
    "qrcode": "^1.5.4"
  }
}
